<html>
  <head>
    <link rel="icon" href="data:;base64,iVBORw0KGgo=" />
    <title>{{$title}}fasthat{{/title}}</title>
    <style>
      .key { color: red; font-weight: bold; }
      .array-collection:before { content: "["; }
      .array-collection:after { content: "]"; }
      .map-collection:before { content: "{"; }
      .map-collection:after { content: "}"; }
      .tuple-collection:before { content: "("; }
      .tuple-collection:after { content: ")"; }
      .collection > .collection-element:not(:last-of-type)::after { content: ", "; }
      .stack-frame .class-name { color: purple; }
      .stack-frame .separator { font-weight: bold; }
    </style>
  </head>

  <body>
    <center><h1>{{$title}}{{/title}}</h1></center>
    {{#snapshot}}
      <form method="POST" action="/dropDump/">
        <input type="submit" value="Unload Dump" />
      </form>
    {{/snapshot}}

    {{$content}}{{/content}}
  </body>
</html>
